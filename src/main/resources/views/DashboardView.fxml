<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eduardamaia.clinica.projetopooclinica.controller.DashboardController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="277.0" layoutY="122.0" style="&#10;" text="Dashboard" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
        <Button fx:id="pacientesButton" layoutX="155.0" layoutY="175.0" mnemonicParsing="false" text="Pacientes" onAction="#handlePacientesButton" />
        <Button fx:id="medicosButton" layoutX="247.0" layoutY="175.0" mnemonicParsing="false" text="Medicos" onAction="#handleMedicosButton" />
        <Button fx:id="consultasButton" layoutX="330.0" layoutY="175.0" mnemonicParsing="false" text="Consultas" onAction="#handleConsultasButton" />
        <Button fx:id="relatoriosButton" layoutX="421.0" layoutY="175.0" mnemonicParsing="false" text="Relatorios" onAction="#handleRelatoriosButton" />
      </children>
    </AnchorPane>
  </children>
