<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eduardamaia.clinica.projetopooclinica.controller.DashboardController">
  <children>
    <VBox alignment="CENTER" VBox.vgrow="ALWAYS"> <children>
      <Label text="Dashboard" textAlignment="CENTER" textFill="#9f9f9f">
        <font>
          <Font size="24.0" /> </font>
        <VBox.margin>
          <Insets bottom="20.0" /> </VBox.margin>
      </Label>
      <HBox alignment="CENTER" spacing="20.0"> <children>
        <Button fx:id="pacientesButton" mnemonicParsing="false" text="Pacientes" onAction="#handlePacientesButton" />
        <Button fx:id="medicosButton" mnemonicParsing="false" text="Médicos" onAction="#handleMedicosButton" />
        <Button fx:id="consultasButton" mnemonicParsing="false" text="Consultas" onAction="#handleConsultasButton" />
        <Button fx:id="relatoriosButton" mnemonicParsing="false" text="Relatórios" onAction="#handleRelatoriosButton" />
      </children>
      </HBox>
    </children>
    </VBox>
  </children>
</VBox>